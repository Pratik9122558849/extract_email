{
  "manifest_version": 3,
  "name": "Cnetinfosystem - Email Extractor",
  "version": "1.1",
  "description": "Extract emails from the current page and save them",
  "permissions": ["scripting", "activeTab", "storage", "tabs", "notifications"],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icon.png"
  }
}
